    "@types/jsonwebtoken": "^8.5.8",
    "@types/ldapjs": "^2.2.2",
    "@types/lodash": "^4.14.199",
    "@types/passport": "^1.0.13",
    "@types/passport-http-bearer": "^1.0.38",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.13",
