  },
  "homepage": "https://github.com/nthngalone/requisite/requisite-api#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.1",
    "express": "^4.18.1",
    "express-mung": "^0.5.1",
    "jsonwebtoken": "^8.5.1",
    "ldapjs": "^2.3.3",
    "log4js": "^6.5.2",
    "passport": "^0.5.3",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
